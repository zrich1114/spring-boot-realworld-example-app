{
  "name": "RealWorld Example App",
  "image": "mcr.microsoft.com/devcontainers/java:11",
  // "build": {
  //   "context": ".",
  //   "dockerfile": "Dockerfile"
  // },
  "features": {
    "ghcr.io/devcontainers/features/github-cli:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": ["vscjava.vscode-java-pack@0.29.2025081500"],
      "settings": {}
    }
  },
  "remoteUser": "vscode",
  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": {
      "label": "RealWorld",
      "onAutoForward": "openBrowser",
      "elevateIfNeeded": true
    }
  }
}
